<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools" android:id="@+id/nav_graph"
            app:startDestination="@id/mainFragment">
    <fragment android:id="@+id/mainFragment" android:name="com.example.takao.androidboilerplate.ui.main.MainFragment"
              android:label="fragment_main" tools:layout="@layout/fragment_main">
        <action android:id="@+id/action_mainFragment_to_nextFragment" app:destination="@id/nextFragment"/>
    </fragment>
    <fragment android:id="@+id/nextFragment" android:name="com.example.takao.androidboilerplate.ui.next.NextFragment"
              android:label="fragment_next" tools:layout="@layout/fragment_next">
        <action
                android:id="@+id/action_nextFragment_to_thirdFragment"
                app:destination="@id/thirdFragment" />
    </fragment>
    <fragment
            android:id="@+id/thirdFragment"
            android:name="com.example.takao.androidboilerplate.ui.third.ThirdFragment"
            android:label="ThirdFragment" />
</navigation>